<?xml version="1.0"?>
<model>
    <model-type>Registration</model-type>

    <template>
        <object id="skull">
            <deformable-object-type>OrientedSurfaceMesh</deformable-object-type>
            <data-sigma>0.01</data-sigma>
            <kernel-width>0.5</kernel-width>
            <kernel-type>exact</kernel-type>
            <filename>data/square1.vtk</filename>
        </object>
    </template>

    <deformation-parameters>
        <kernel-width>0.3</kernel-width>
        <kernel-type>exact</kernel-type>
        <number-of-timepoints>6</number-of-timepoints>
    </deformation-parameters>
</model>
