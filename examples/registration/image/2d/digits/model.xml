<?xml version="1.0"?>
<model>

    <model-type>Registration</model-type>
    <template>
        <object id="digit">
            <deformable-object-type>SSDImage</deformable-object-type>
            <data-sigma>0.1</data-sigma>
            <filename>data/digit_2_sample_10.png</filename>
        </object>
    </template>

    <deformation-parameters>
        <kernel-width>3</kernel-width>
        <kernel-type>exact</kernel-type>
        <number-of-timepoints>20</number-of-timepoints>
    </deformation-parameters>

</model>
