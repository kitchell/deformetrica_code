<?xml version="1.0"?>

<SPARSE-DIFFEO-PARAMETERS>

    <ATLAS-TYPE>DETERMINISTIC</ATLAS-TYPE>  <!-- BAYESIAN or DETERMINISTIC -->
    <KERNEL-TYPE>cudaexact</KERNEL-TYPE> <!-- cudaexact, exact -->
    <KERNEL-WIDTH>11</KERNEL-WIDTH> <!-- big=fast and not so accurate / small=slow and accurate -->
    <INITIAL-STEP-MULTIPLIER>0.001</INITIAL-STEP-MULTIPLIER>
    <OPTIMIZATION-METHOD-TYPE>gradientdescent</OPTIMIZATION-METHOD-TYPE> <!-- gradientdescent or FISTA -->
    <MAX-ITERATIONS>1</MAX-ITERATIONS>
    <NUMBER-OF-THREADS>10</NUMBER-OF-THREADS>

    <!-- Parameters by default  -->
    <MAX-LINE-SEARCH-ITERATIONS>10</MAX-LINE-SEARCH-ITERATIONS>
    <NUMBER-OF-TIMEPOINTS>10</NUMBER-OF-TIMEPOINTS>
    <ADAPTIVE-TOLERANCE>1e-4</ADAPTIVE-TOLERANCE>
    <CP-AT-SHAPE-POINTS>OFF</CP-AT-SHAPE-POINTS>
    <SMOOTHING-KERNEL-WIDTH-RATIO>0.5</SMOOTHING-KERNEL-WIDTH-RATIO>
    <P3M-WORKING-SPACING-RATIO>0.2</P3M-WORKING-SPACING-RATIO>
    <P3M-PADDING-FACTOR>3.0</P3M-PADDING-FACTOR>
    <SPARSITY-PRIOR>0.0</SPARSITY-PRIOR>
    <STEP-EXPAND>1.5</STEP-EXPAND>
    <STEP-SHRINK>0.5</STEP-SHRINK>
    <ATLAS-NAME>Atlas</ATLAS-NAME>
    <FREEZE-TEMPLATE>OFF</FREEZE-TEMPLATE>
    <INITIAL-CP-SPACING></INITIAL-CP-SPACING>
    <COVARIANCE-MOMENTA-NORMALIZED-HYPERPARAMETER>0.001</COVARIANCE-MOMENTA-NORMALIZED-HYPERPARAMETER>
    <FREEZE-CP>Off</FREEZE-CP>
    <INITIAL-CP-POSITION></INITIAL-CP-POSITION>
    <INITIAL-MOMENTA></INITIAL-MOMENTA>
    <COVARIANCE-MOMENTA-PRIOR-INVERSE-FN></COVARIANCE-MOMENTA-PRIOR-INVERSE-FN>
    <COVARIANCE-MOMENTA-INVERSE-FN></COVARIANCE-MOMENTA-INVERSE-FN>

</SPARSE-DIFFEO-PARAMETERS>












