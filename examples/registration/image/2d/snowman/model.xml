<?xml version="1.0"?>
<model>

    <model-type>Registration</model-type>

    <template>
        <object id="img">
            <deformable-object-type>EQLAImage</deformable-object-type>
            <data-sigma>0.5</data-sigma>
            <kernel-width>40.0</kernel-width>
            <kernel-type>exact</kernel-type>
            <image-grid-downsampling>2</image-grid-downsampling>
            <filename>data/I1.png</filename>
        </object>
    </template>

    <deformation-parameters>
        <kernel-width>15.0</kernel-width>
        <kernel-type>p3m</kernel-type>
    </deformation-parameters>

</model>
