<?xml version="1.0"?>
<model>

    <model-type>BayesianAtlas</model-type>

    <template>
        <object id="img">
            <deformable-object-type>SSDImage</deformable-object-type>
            <filename>data/digit_2_mean.png</filename>
        </object>
    </template>

    <deformation-parameters>
        <kernel-width>3</kernel-width>
        <kernel-type>cudaexact</kernel-type>
        <number-of-timepoints>10</number-of-timepoints>
    </deformation-parameters>

</model>
