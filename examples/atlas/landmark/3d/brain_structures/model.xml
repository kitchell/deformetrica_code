<?xml version="1.0"?>
<model>
    <model-type>BayesianAtlas</model-type>
    <template>
        <object id="amygdala">
            <deformable-object-type>NonOrientedSurfaceMesh</deformable-object-type>
            <data-sigma>10.0</data-sigma>
            <kernel-type>cudaexact</kernel-type>
            <kernel-width>15</kernel-width>
            <filename>data/amyg_prototype.vtk</filename>
        </object>
        <object id="hippo">
            <deformable-object-type>NonOrientedSurfaceMesh</deformable-object-type>
            <data-sigma>10.0</data-sigma>
            <kernel-type>cudaexact</kernel-type>
            <kernel-width>15</kernel-width>
            <filename>data/hippo_prototype.vtk</filename>
        </object>
    </template>
    <deformation-parameters>
        <kernel-width>10.0</kernel-width>
        <kernel-type>cudaexact</kernel-type>
    </deformation-parameters>
</model>
